{
    "name": "mangoweb-sylius/sylius-extended-channels",
    "type": "sylius-plugin",
    "keywords": ["sylius", "sylius-plugin"],
    "description": "Extended channels plugin for Sylius",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "php": "^8.2",
        "sylius/sylius": "2.0.*"
    },
    "require-dev": {
        "behat/behat": "^3.16",
        "dmore/behat-chrome-extension": "^1.4",
        "dmore/chrome-mink-driver": "^2.9",
        "friends-of-behat/mink": "^1.11",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "friends-of-behat/mink-debug-extension": "^2.1",
        "friends-of-behat/mink-extension": "^2.7",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.1",
        "friends-of-behat/symfony-extension": "^2.6",
        "friends-of-behat/variadic-extension": "^1.6",
        "nyholm/psr7": "^1.8",
        "phpspec/phpspec": "^7.5",
        "phpstan/phpstan": "^1.12",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-webmozart-assert": "^1.2",
        "phpunit/phpunit": "^10.5",
        "robertfausk/behat-panther-extension": "^1.1",
        "sylius-labs/coding-standard": "^4.4",
        "sylius-labs/suite-tags-extension": "~0.2",
        "sylius/sylius-rector": "^2.0",
        "symfony/browser-kit": "^6.4 || ^7.1",
        "symfony/debug-bundle": "^6.4 || ^7.1",
        "symfony/dotenv": "^6.4 || ^7.1",
        "symfony/flex": "^2.4",
        "symfony/http-client": "^6.4 || ^7.1",
        "symfony/intl": "^6.4 || ^7.1",
        "symfony/web-profiler-bundle": "^6.4 || ^7.1",
        "symfony/webpack-encore-bundle": "^2.2"
    },
    "conflict": {
        "api-platform/symfony": "<v4.1.0",
        "api-platform/core": "<2.7.13",
        "babdev/pagerfanta-bundle": "<3.6.1",
        "doctrine/collections": "<1.7.0",
        "doctrine/data-fixtures": "<1.5.1",
        "doctrine/dbal": "<2.13.3",
        "doctrine/doctrine-bundle": "<2.8.0",
        "friendsofsymfony/rest-bundle": "<3.1.0",
        "jms/serializer-bundle": "<4.2.0",
        "masterminds/html5": "<2.7.5",
        "knplabs/knp-menu": "<3.3.0",
        "lexik/jwt-authentication-bundle": "<2.12",
        "payum/core": "<1.7.3",
        "polishsymfonycommunity/symfony-mocker-container": "<1.0.6",
        "symfony/css-selector": "<4.4.24",
        "symfony/framework-bundle": ">=5.4.0 <=5.4.20|>=6.0.0 <=6.0.16|>=6.1.0 <=6.1.8|>=6.2.0 <=6.2.2|6.2.8",
        "symfony/mime": "<5.4.0",
        "symfony/web-link": "<5.3.0",
        "symplify/easy-coding-standard": "<10.2.0",
        "sylius/sylius": "<2.0.6",
        "twig/twig": "<2.14.7",
        "webmozart/assert": "<1.11.0",
        "willdurand/negotiation": "<3.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "MangoSylius\\ExtendedChannelsPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MangoSylius\\ExtendedChannelsPlugin\\": "tests/Application/src/",
            "Tests\\MangoSylius\\ExtendedChannelsPlugin\\Behat\\": "tests/Behat/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "symfony": {
            "require": "^6.4"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "php-http/discovery": false,
            "phpstan/extension-installer": true,
            "symfony/flex": true
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-update-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-create-project-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "COMPOSER_AUDIT_ABANDONED=ignore composer audit": "script"
        }
    }
}
